{
  "name": "whatsapp-widget",
  "version": "1.0.0",
  "description": "Whatsapp chat widget for your store.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agraris/whatsapp-widget.git"
  },
  "keywords": [
    "whatsapp",
    "wa",
    "widget",
    "html",
    "css",
    "javascript",
    "chat"
  ],
  "author": "Fajar Setya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agraris/whatsapp-widget/issues"
  },
  "homepage": "https://github.com/agraris/whatsapp-widget#readme",
  "scripts": {
    "compile": "npm-run-all --parallel compile-*",
    "compile-css": "node-sass --output-style expanded --precision 6 scss/default.scss dist/css/default.css",
    "compile-js": "rollup --config",
    "test": "jest",
    "watch": "npm-run-all --parallel watch-*",
    "watch-css": "nodemon --watch scss/ --ext scss --exec \"npm run compile-css\"",
    "watch-js": "nodemon --watch src/ --ext js --exec \"npm run compile-js\""
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-babel": "^5.1.0",
    "autoprefixer": "^9.8.5",
    "csso-cli": "^3.0.0",
    "jest": "^26.0.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "rollup": "^2.21.0"
  }
}
